{
  "name": "juralen-elm",
  "author": "Lindsay Wardell",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "elm-tooling install",
    "dev": "npm run build-analyzer && vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "server": "nodemon -e js,ts,elm --exec 'vite-node' server.ts",
    "prebuild": "elm-tooling install",
    "build": "npm run build-analyzer && vite build",
    "build-analyzer": "vite build --config analyzer.config.ts",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.3",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.8",
    "elm-tooling": "^1.0.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^2.2.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.5.1",
    "vite-node": "^0.1.11",
    "vite-plugin-elm": "^2.4.3",
    "vite-plugin-pwa": "^0.11.2"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.2.26",
    "express": "^4.17.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.6"
  }
}
